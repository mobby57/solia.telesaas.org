{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": "./",
    "types": ["node"]
  },
  "include": ["src", "../solia-test"],
  "exclude": ["node_modules", "dist"]
}
