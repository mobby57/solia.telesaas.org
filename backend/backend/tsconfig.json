{
  "compilerOptions": {
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "target": "ES2021",
    "module": "ESNext",
    "lib": ["es2021"],
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "solia-test"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
